{
  "name": "Godot Theme",
  "author": "Elliot Waite",
  "editorScheme": "/godot_theme.xml",
  "ui": {
    "*": {
      "background": "MEDIUM_BLUE",
      "foreground": "LIGHTER_GREY",

      "infoForeground": "MEDIUM_GREY",

      "selectionBackground": "DARK_GREY",
      "selectionInactiveBackground": "DARKER_GREY",
      "selectionBackgroundInactive": "DARKER_GREY",
      "selectionForeground": "ALMOST_WHITE",

      "disabledBackground": "MEDIUM_BLUE",
      "inactiveBackground": "MEDIUM_BLUE",

      "acceleratorForeground": "ALMOST_WHITE",
      "acceleratorSelectionForeground": "ALMOST_WHITE",

      "errorForeground": "ERROR",

      "borderColor": "DARKER_BLUE",
      "disabledBorderColor": "DARKER_BLUE",
      "focusColor": "DARKER_BLUE",
      "focusedBorderColor": "LIGHTER_BLUE",

      "separatorColor": "MEDIUM_GREY"
    },

    "Borders": {
      "color": "DARKER_BLUE",
      "ContrastBorderColor": "DARKER_BLUE"
    },

    "DefaultTabs": {
      "underlineColor": "LIGHTER_GREY",
      "inactiveUnderlineColor": "MEDIUM_GREY",
      "hoverBackground": "DARK_BLUE"
    },

    "Editor": {
      "background": "DARK_BLUE",
      "foreground": "LIGHTER_GREY",
      "shortcutForeground": "LIGHTER_BLUE"
    },

    "EditorPane.inactiveBackground": "DARK_BLUE",

    "EditorTabs": {
      "underlinedTabBackground": "LIGHT_BLUE"
    },

    "TextArea": {
      "background": "DARK_BLUE",
      "selectionBackground": "LIGHT_BLUE"
    },

    "TextField": {
      "background": "DARK_BLUE",
      "selectionBackground": "LIGHT_BLUE"
    },

    "ToolWindow": {
      "Button": {
        "hoverBackground": "DARK_BLUE",
        "selectedBackground": "LIGHT_BLUE",
        "selectedForeground": "LIGHTER_GREY"
      },

      "Header": {
        "background": "LIGHT_BLUE",
        "inactiveBackground": "MEDIUM_BLUE"
      }
    },

    "Tree": {
      "selectionBackground": "DARK_GREY"
    }
  }
}
